pipeline {
    agent any 
    environment {
        BUILD_NUMBER = 'LATEST'
        registry = 'vmahecha07/nodefront'
        registry2 = 'vmahecha07/nodeapp'
        registryCredential = 'vmahecha07'
        dockerImage = ''
    }
    stages {
        stage('clone') {
            steps {
                echo 'clone the repo'
                sh 'rm -fr rp2'
                git branch: 'main', url: 'https://github.com/vmahecha/deploy.git'
                sh 'pwd'
                sh 'ls'
            }
        }
    }
}